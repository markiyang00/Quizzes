@model Test
<div class="row">
	<h3>Add Test</h3>
	<form asp-action="AddTest" enctype="multipart/form-data" method="post" class="form-horizontal">
		<br />
		<br />
		<div class="form-group">
			<label asp-for="Name"></label>
			<input asp-for="Name" class="from-control" />
			<span asp-validation-for="Name" class="text-danger"></span>
		</div>
		<div class="form-group">
			<label asp-for="TestTime"></label>
			<input asp-for="TestTime" class="from-control" />
			<span asp-validation-for="TestTime" class="text-danger"></span>
		</div>
		<input class="btn btn-success" type="submit" value="AddTest" />
	</form>
</div>