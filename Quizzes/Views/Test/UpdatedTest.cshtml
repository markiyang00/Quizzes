@model Test
<div class="row">
	<h3>Edit Test</h3>
	<form asp-action="UpdatedTest" enctype="multipart/form-data" method="post" class="form-horizontal">
		<br />
		<br />
		<div type="hidden" class="form-group">
			<div type="hidden" class="col-md-5">
				<input type="hidden" asp-for="Id" class="from-control" />
				<span type="hidden" asp-validation-for="Id" class="text-danger"></span>
			</div>
		</div>
		<div class="form-group">
			<label asp-for="Name"></label>
			<input asp-for="Name" class="from-control" />
			<span asp-validation-for="Name" class="text-danger"></span>
		</div>
		<div class="form-group">
			<label asp-for="TestTime"></label>
			<input asp-for="TestTime" class="from-control" />
			<span asp-validation-for="TestTime" class="text-danger"></span>
		</div><br/>
		<input class="btn btn-success" type="submit" value="Edit Test" />
	</form>
</div>