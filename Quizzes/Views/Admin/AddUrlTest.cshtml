@model UrlTest
<div class="row">
	<h3>Add Url</h3>
	<form asp-action="AddUrlTest" enctype="multipart/form-data" method="post" class="form-horizontal">
		<br />
		<br />
		<div type="hidden" class="form-group">
			<div type="hidden" class="col-md-5">
				<input type="hidden" asp-for="TestId" class="from-control" />
				<span type="hidden" asp-validation-for="TestId" class="text-danger"></span>
			</div>
		</div>
		<div class="form-group">
			<label asp-for="Time"></label>
			<input asp-for="Time" class="from-control">
			<span asp-validation-for="Time" class="text-danger"></span>
		</div>
		<div class="form-group">
			<label asp-for="NumberOfRuns"></label>
			<input asp-for="NumberOfRuns" class="from-control" />
			<span asp-validation-for="NumberOfRuns" class="text-danger"></span>
		</div>
		<div class="form-group">
			<label asp-for="Url"></label>
			<input asp-for="Url" class="from-control" />
			<span asp-validation-for="Url" class="text-danger"></span>
		</div>
		<div class="form-group">
			<label asp-for="Name"></label>
			<input asp-for="Name" class="from-control" />
			<span asp-validation-for="Name" class="text-danger"></span>
		</div><br/>
		<input class="btn btn-success" type="submit" value="Add Url" />
	</form>
</div>